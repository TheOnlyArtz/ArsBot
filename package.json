{
  "name": "arsbot",
  "version": "2.0.1",
  "description": "ArsBot by Ronen and Amit",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "xo": {
    "space": 4,
    "semicolon": true,
    "rules": {
      "camelcase": 0,
      "new-parens": 0,
      "guard-for-in": 0,
      "prefer-const": 0,
      "no-eval": 0,
      "no-unused-vars": 0,
      "no-prototype-builtins": 0,
      "unicorn/filename-case": 0,
      "import/no-dynamic-require": 0
    },
    "ignores": [
      "tests/**"
    ]
  },
  "author": "TheOnlyArtz",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.2",
    "chalk": "^1.1.3",
    "cheerio": "^1.0.0-rc.1",
    "cli-table": "^0.3.1",
    "cowsay": "^1.1.9",
    "discord.js": "github:hydrabolt/discord.js",
    "knex": "^0.13.0",
    "moment": "^2.18.1",
    "mongodb": "^2.2.28",
    "ms": "^2.0.0",
    "mysql": "^2.13.0",
    "node-fetch": "^1.7.1",
    "nodemon": "^1.11.0",
    "opusscript": "0.0.3",
    "overwatch-stats": "^3.0.2",
    "random-puppy": "^1.1.0",
    "request": "^2.81.0",
    "respawn": "^2.5.0",
    "snekfetch": "^3.1.11",
    "sqlite": "^2.8.0",
    "winston": "^2.3.1",
    "ytdl-core": "^0.14.4"
  },
  "devDependencies": {
    "standard": "^10.0.2",
    "xo": "^0.18.0"
  }
}
